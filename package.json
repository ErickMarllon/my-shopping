{
  "name": "my-shopping",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite --mode dev",
    "start:dev": "vite --mode dev",
    "start:loc": "vite --mode loc",
    "start:hlg": "vite --mode hlg",
    "start:prod": "vite --mode prod",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "check-updates": "npx npm-check-updates",
    "updates": "npx npm-check-updates -u",
    "typesync": "npx typesync",
    "format": "yarn prettier && yarn lint",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix",
    "prepare": "husky install",
    "plop": "plop",
    "check-env": "ts-node ./env/validation/checkEnvFiles.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.11",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.7",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tanstack/eslint-plugin-query": "^5.66.1",
    "@types/bootstrap": "~5.2.10",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/node": "^22.13.7",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "plop": "^4.0.1",
    "prettier": "^3.5.2",
    "sass": "^1.85.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
